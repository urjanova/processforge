{
  "metadata": {
    "name": "Hydraulic Chain Example",
    "description": "Base configuration for a two-pump, one-valve, one-strainer hydraulic system.",
    "version": "1.0"
  },
  "streams": {
    "feed": {
      "T": 298.15,
      "P": 101325,
      "z": {"Water": 1.0},
      "flowrate": 1.0
    }
  },
  "units": {
    "pump_1": {
      "type": "Pump",
      "in": "feed",
      "out": "after_pump_1",
      "deltaP": 200000.0,
      "efficiency": 0.75
    },
    "valve_1": {
      "type": "Valve",
      "in": "after_pump_1",
      "out": "after_valve_1",
      "pressure_ratio": 0.5
    },
    "strainer_1": {
      "type": "Strainer",
      "in": "after_valve_1",
      "out": "after_strainer_1",
      "deltaP": 5000.0
    },
    "pump_2": {
      "type": "Pump",
      "in": "after_strainer_1",
      "out": "product",
      "deltaP": 300000.0,
      "efficiency": 0.8
    }
  },
  "simulation": {
    "mode": "steady"
  }
}
